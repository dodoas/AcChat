<h1>Open new account</h1>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<div class="form-group">
    <%= f.text_field :first_name, required: true, autofocus: true, :class => 'form-control', :placeholder => 'First_name' %>
    </div>
    <div class="form-group">
    <%= f.text_field :last_name, required: true, autofocus: true, :class => 'form-control', :placeholder => 'Last name' %>
    </div>
  <div class="form-group">
    <%= f.email_field :email, required: true, autofocus: true, :class => 'form-control', :placeholder => 'Email address' %>
    </div>
    <div class="form-group">
    <%= f.password_field :password, required: true, hint: ("#{@minimum_password_length} characters minimum" if @validatable), :class => 'form-control', :placeholder => 'Password' %>
    </div>
    <div class="form-group">
    <%= f.password_field :password_confirmation, required: true, :class => 'form-control', :placeholder => 'Password confirmation' %>
  </div>
  <div class="form-group">
    <%= f.button "Sign up", :class => 'btn btn-custom btn-lg btn-block' %>
  </div>
<% end %>
<div class="form-group">
<%= render "devise/shared/links" %>
</div>